	urldate = {2010-12-30},
	journal = {Optics Communications},
	author = {Rother, Tom},
	month = jul,
	year = {2005},
	keywords = {Acoustic scattering, Green's function, Helmholtz equation, Light scattering, Vector wave equation},
	pages = {270--285},
}

