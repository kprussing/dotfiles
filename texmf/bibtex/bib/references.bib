	author = {LeVine, Susan},
	month = jun,
	year = {2009},
	keywords = {*{NONLETHAL} {WEAPONS}, *{REFLEXES}, {ACTIVE} {DENIAL} {TECHNOLOGIES}, {ADS}({ACTIVE} {DENIAL} {SYSTEM}), {ANATOMY} {AND} {PHYSIOLOGY}, {ANTIPERSONNEL} {EFFECTS}, {DIRECTED} {ENERGY} {WEAPONS}, {EYE}, {GYROTRONS}, {MILLIMETER} {WAVE} {EQUIPMENT}, {RANGE}({DISTANCE}), {SKIN}({ANATOMY}), {TECHNOLOGY} {ASSESSMENT}, {WEAPONS} {EFFECTS}({BIOLOGICAL})},
