@article{foster_automatic_2003,
	title = {Automatic gait recognition using area-based metrics},
	volume = {24},
	issn = {0167-8655},
	url = {http://www.sciencedirect.com/science/article/B6V15-48TMJSN-1/2/9a852496d2b43dab2110b6202ac084f2},
	doi = {10.1016/S0167-8655(03)00094-1},
	abstract = {{\textless}p{\textgreater}{\textless}br/{\textgreater}A novel technique for analysing moving shapes is presented in an example application to automatic gait recognition. The technique uses masking functions to measure area as a time varying signal from a sequence of silhouettes of a walking subject. Essentially, this combines the simplicity of a baseline area measure with the specificity of the selected (masked) area. The dynamic temporal signal is used as a signature for automatic gait recognition. The approach is tested on the largest extant gait database, consisting of 114 subjects (filmed under laboratory conditions). Though individual masks have limited discriminatory ability, a correct classification rate of over 75\% was achieved by combining information from different area masks. Knowledge of the leg with which the subject starts a gait cycle is shown to improve the recognition rate from individual masks, but has little influence on the recognition rate achieved from combining masks. Finally, this technique is used to attempt to discriminate between male and female subjects. The technique is presented in basic form: future work can improve implementation factors such as using better data fusion and classifiers with potential to increase discriminatory capability.{\textless}/p{\textgreater}},
	number = {14},
	urldate = {2011-05-05},
	journal = {Pattern Recognition Letters},
