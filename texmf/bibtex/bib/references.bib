@article{hollins_dark_1973,
	title = {Dark Adaptation and Visual Pigment Regeneration in Human Cones},
	volume = {62},
	url = {http://jgp.rupress.org/content/62/4/430.abstract},
	doi = {10.1085/jgp.62.4.430},
	abstract = {Foveal threshold elevation and red-green cone pigment regeneration have been studied in the dark after a wide range of bleaches in normal man with a view to probing the limits of the application of the Dowling-Rushton relation: i.e., the direct proportionality between log threshold elevation and fraction of unregenerated pigment. Cone pigment regeneration (and threshold recovery) is much faster after short bleaches than expected from the kinetics of a simple monomolecular reaction. Recovery is faster after a fixed (short) duration bleach the weaker it is. Except for the first 30 s after relatively weak bleaches and the entire recovery after a very brief ({\textless}0.001 s) saturating bright flash which bleaches a little more than 50\%, the results are accurately fit by the Dowling-Rushton relation over the entire range tested with only one arbitrary constant (the proportionality factor). Theory predicts too low threshold in comparison with what is obtained, for both of these exceptions},
