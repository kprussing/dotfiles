	year = {1999},
	keywords = {background clutter, camouflage nets, detection performance, Dixie data collection, false alarm rate, feature extraction, foliage penetration radar, {FOPEN}, hyperspectral imaging systems, identification performance, image coregistration, leakage rate, material identification, military operations, military radar, Mississippi, principal component analysis, principal components, radar applications, radar cross section, radar cross-sections, radar detection, radar imaging, {RCS}, {SAR} image, {SAR}/{HSI} data fusion, {SENSOR} {FUSION}, spectral analysis, spectral feature extraction, spectral measurements, synthetic aperture radar, target detection, tree canopies, vehicle paints, Vicksberg},
