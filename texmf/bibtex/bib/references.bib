@article{siegman_excess_1989-1,
	title = {Excess spontaneous emission in non-Hermitian optical systems. {II}. Laser oscillators},
	volume = {39},
	url = {http://link.aps.org.www.library.gatech.edu:2048/doi/10.1103/PhysRevA.39.1264},
