	doi = {10.1016/j.jcp.2006.08.021},
	abstract = {The lattice Boltzmann method ({LBM}) was used to solve the energy equation of a transient conduction-radiation heat transfer problem. The finite volume method ({FVM}) was used to compute the radiative information. To study the compatibility of the {LBM} for the energy equation and the {FVM} for the radiative transfer equation, transient conduction and radiation heat transfer problems in 1-D planar and 2-D rectangular geometries were considered. In order to establish the suitability of the {LBM}, the energy equations of the two problems were also solved using the {FVM} of the computational fluid dynamics. The {FVM} used in the radiative heat transfer was employed to compute the radiative information required for the solution of the energy equation using the {LBM} or the {FVM} (of the {CFD}). To study the compatibility and suitability of the {LBM} for the solution of energy equation and the {FVM} for the radiative information, results were analyzed for the effects of various parameters such as the scattering albedo, the conduction-radiation parameter and the boundary emissivity. The results of the {LBM}-{FVM} combination were found to be in excellent agreement with the {FVM}-{FVM} combination. The number of iterations and {CPU} times in both the combinations were found comparable.},
	number = {1},
	urldate = {2010-06-02},
	journal = {Journal of Computational Physics},
	author = {Mishra, Subhash C. and Roy, Hillol K.},
	month = apr,
	year = {2007},
	keywords = {Conduction-radiation, Finite volume method, Lattice Boltzmann method, Participating medium, Radiative heat transfer},
