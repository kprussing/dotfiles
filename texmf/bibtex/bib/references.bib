@article{chong_coherent_2010,
	title = {Coherent Perfect Absorbers: Time-Reversed Lasers},
	volume = {105},
	shorttitle = {Coherent Perfect Absorbers},
	url = {http://link.aps.org.www.library.gatech.edu:2048/doi/10.1103/PhysRevLett.105.053901},
